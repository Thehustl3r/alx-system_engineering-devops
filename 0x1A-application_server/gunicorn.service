# /etc/systemd/system/gunicorn.service
  
[Unit]
Description=Gunicorn service for AirBnb clone

[Service]
ExecStart=/usr/bin/gunicorn -b 0.0.0.0:5003 -W 3 -t 60 --error-logfile=/tmp/airbnb-access.log web_dynamic.2-hbnb:app
Restart=always
User=ubuntu
Group=ubuntu

[Install]
WantedBy=multi-user.target
